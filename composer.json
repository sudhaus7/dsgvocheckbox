{
  "name": "sudhaus7/formframework",
  "description": "(Sudhaus7) Form Framework Tools and Extensions",
  "version": "1.2.3",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Frank Berger",
      "role": "Developer"
    }
  ],
  "replace": {
    "sudhaus7_formframework": "self.version"
  },
  "require": {
    "php": ">=7.0",
    "typo3/cms-core": "~7.6.23 | ~8.7.10 | ~9.5.9 | ~10.4",
    "typo3/cms-form": "~7.6.23 | ~8.7.10 | ~9.5.9 | ~10.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "typo3/cms-fluid-styled-content": "^7.6.23 | ^8.7.10 | ^9.5.9 | ^10.4",
    "helhum/typo3-composer-setup": "^0.5.1",
    "helmich/typo3-typoscript-lint": "^1.4.4",
    "nimut/testing-framework": "^5.2",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "sudhaus7_formframework",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"
    }
  },
  "autoload": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Formframework\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Formframework\\Tests\\": "Tests/"
    }
  }
}
