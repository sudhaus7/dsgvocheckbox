{
  "name": "sudhaus7/formframework",
  "description": "(Sudhaus7) Form Framework Tools and Extensions",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "license": "",
  "authors": [
    {
      "name": "Frank Berger",
      "role": "Developer"
    }
  ],
  "replace": {
    "sudhaus7_formframework": "self.version"
  },
  "require": {
    "php": "~7.0.0 || ~7.1.0 || ~7.2.0",
    "typo3/cms-core": ">=7.6,<8.9",
    "typo3/cms-form": ">=7.6,<8.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "typo3/cms-fluid-styled-content": "^7.6.23 || ^8.7.10",
    "helhum/typo3-composer-setup": "^0.5.1",
    "helmich/typo3-typoscript-lint": "^1.4.4",
    "nimut/testing-framework": "^3.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "vendor-dir": ".Build/vendor"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "sudhaus7_formframework",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"
    }
  },
  "autoload": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Formframework\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Formframework\\Tests\\": "Tests/"
    }
  }
}
